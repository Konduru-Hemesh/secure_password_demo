{
    "extends": "./tsconfig.app.json",
    "compilerOptions": {
        "composite": true,
        "noEmit": false,
        "allowImportingTsExtensions": false,
        "outDir": "./node_modules/.tmp/test",
        "types": [
            "vitest/globals",
            "@testing-library/jest-dom"
        ],
        "noUnusedLocals": false,
        "noUnusedParameters": false
    },
    "include": [
        "src/**/*.ts",
        "src/**/*.tsx",
        "src/**/*.test.ts",
        "src/**/*.test.tsx",
        "src/**/*.spec.ts",
        "src/**/*.spec.tsx"
    ],
    "exclude": [
        "node_modules"
    ]
}